		"otpauth": "^9.3.6",
		"payload": "^3.3.0",
		"powerhooks": "^1.0.20",
		"react": "19.0.0-rc-65a56d0e-20241020",
		"react-dom": "19.0.0-rc-65a56d0e-20241020",
		"react-transition-group": "^4.4.5",
		"remark": "^14.0.3",
