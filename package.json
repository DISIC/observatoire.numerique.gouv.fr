{
	"name": "observatoire",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "only-include-used-icons && next dev",
		"build": "next build",
		"start": "next start",
		"lint": "prettier --check .",
		"payload": "PAYLOAD_CONFIG_PATH=./src/payload/payload.config.ts payload",
		"seed:dev": "PAYLOAD_DROP_DATABASE=true tsx ./src/payload/seed/index.ts --dev",
		"seed:prod": "tsx ./src/payload/seed/index.ts",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"format": "prettier --write .",
		"predev": "only-include-used-icons",
		"prebuild": "only-include-used-icons",
		"lhci:audit": "next build && concurrently \"npm run lhci:server\" \"lhci autorun\"",
		"lhci:wizard": "lhci wizard",
		"lhci:server": "lhci server --storage.storageMethod=sql --storage.sqlDialect=sqlite --storage.sqlDatabasePath=./db.sql"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "prettier --write"
	},
	"prisma": {
		"seed": "tsx ./prisma/seed.ts"
	},
	"dependencies": {
		"@codegouvfr/react-dsfr": "^1.17.0",
		"@emotion/react": "^11.10.6",
		"@emotion/server": "^11.10.0",
