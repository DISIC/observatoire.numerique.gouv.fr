		"@payloadcms/storage-s3": "^3.5.0",
		"@prisma/client": "^4.11.0",
		"@socialgouv/matomo-next": "^1.6.1",
		"@tanstack/react-query": "4",
		"@trpc/client": "^10.45.2",
		"@trpc/next": "^10.45.2",
		"@trpc/react-query": "10",
		"@trpc/server": "^10.45.2",
		"@types/node": "^20.0.0",
		"@types/react-transition-group": "^4.4.5",
		"airtable": "^0.11.6",
		"bcrypt": "^5.1.1",
		"cookies-next": "^5.0.2",
