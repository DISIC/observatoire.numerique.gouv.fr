{
	"name": "observatoire",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "prettier --check .",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"format": "prettier --write .",
		"predev": "only-include-used-icons",
		"prebuild": "only-include-used-icons",
		"lhci:audit": "next build && concurrently \"npm run lhci:server\" \"lhci autorun\"",
		"lhci:wizard": "lhci wizard",
		"lhci:server": "lhci server --storage.storageMethod=sql --storage.sqlDialect=sqlite --storage.sqlDatabasePath=./db.sql"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "prettier --write"
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
	},
	"dependencies": {
		"@codegouvfr/react-dsfr": "^0.48.0",
		"@emotion/react": "^11.10.6",
		"@emotion/server": "^11.10.0",
		"@next/font": "13.1.6",
		"@prisma/client": "^4.11.0",
		"@types/node": "^18.15.7",
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"@types/react-transition-group": "^4.4.5",
		"airtable": "^0.11.6",
		"eslint": "8.33.0",
		"eslint-config-next": "13.1.6",
		"focus-trap-react": "^10.1.1",
		"next": "13.1.6",
		"next-auth": "^4.20.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-transition-group": "^4.4.5",
		"sass": "^1.58.3",
		"superjson": "^1.12.2",
		"swr": "^2.1.1",
		"tss-react": "^4.7.4",
		"typescript": "^5.0.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/plugin-transform-modules-commonjs": "^7.21.2",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.0",
		"@lhci/cli": "^0.11.0",
		"@lhci/server": "^0.11.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"babel-jest": "^29.5.0",
		"concurrently": "^7.6.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"prettier": "^2.8.4",
		"prisma": "^4.11.0",
		"react-test-renderer": "^18.2.0",
		"sqlite3": "^5.1.4",
		"ts-node": "^10.9.1"
	}
}
